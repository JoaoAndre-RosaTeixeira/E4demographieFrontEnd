{
  "hosting": {
    "site": "dev-ia-e4-fronted",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "buildCommand": "npm run build",
      "publicDirectory": "dist"
    },
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
